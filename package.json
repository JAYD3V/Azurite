{
    "name": "azurite",
    "version": "0.0.0",
    "publisher": "JAYD3V (A.J. Chambers)",
    "displayName": "Azurite",
    "description": "A blue theme that resembles the Azurite Minerals found deep in the earths crust",
    "preview": true,
    "license": "Apache-2.0 & CC-4.0 by NC ND",
    "galleryBanner": {
        "color": "#113366",
        "theme": "dark"
    },
    "repository": "git+https://github.com/W3Dojo/UI-Dynamics.git",
    "bugs": "https://github.com/W3Dojo/UI-Dynamics/issues",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": ["Themes"],
    "keywords": [
        "VS Code", "Color", "Theme", "Icon", "Icons", "Product-Icons", "Workbench", "Color-customizations", "Dark", "Blue", "Grey", "Silver", "Azure", "Azurite", "Titanium", "Metalic", "Navy", "Onyx", "Oxford", "Theme-Pack", "Color & Icon", "JAYD3V", "jD3V", "JAYDEV", "jDev"
    ],
    "contributes": {
        "themes": [
            {
                "id": "azurite-vscode-theme",
                "label": "VSC THEME — AZURITE",
                "uiTheme": "vs-dark",
                "path": "./vscode-theme/azurite-vscode-theme.json"
            },
            {
                "id": "titanium-vscode-theme",
                "label": "VSC THEME — TITANIUM",
                "uiTheme": "vs-dark",
                "path": "./vscode-theme/titanium-vscode-theme.json"
            }
        ],

        "productIconThemes": [
            {
                "path": "./themes/azurite-product-icon-theme.json",
                "label": "Azurite Product Icons",
                "id": "azurite"
            }
        ]
    },

    "scripts": {
        "start": "node ./build/updateFont.js",
        "see-font": "code ./theme/vscode-10.html"
    },

    "publishConfig": {
        "access": "public"
    },

    "devDependencies": {
        "vsce": "^2.7.0",
        "open": "^7.0.3",
        "webfont": "^11.2.4"
    }
}
