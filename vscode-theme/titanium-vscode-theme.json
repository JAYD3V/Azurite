{
   "$schema": "vscode://schemas/color-theme",
   "type": "dark",
   "semanticHighlighting": true,
   "tokenColors": [
      {
         // This array object is sets the color that is assigned to every property in
         // the theme. When an entity is theme-able, but it does not have a selector targeting it, or its selector, and any fallback selectors have failed  (what ever the reason may be), this will be the color that the entity will fallback to
         "name": "EDITOR FG",
         "scope": ["source", "text"],
         "settings": { "foreground": "#C3C3C3", "fontStyle": "" }
      },

      {
         "name": "COMMENT",
         "scope": ["comment", "punctuation.definition.comment"],
         "settings": { "foreground": "#508296", "fontStyle": "" }
      },

      {
         "name": "PUNCTUATION",
         "scope": ["punctuation", "meta.brace"],
         "settings": { "foreground": "#9F9284", "fontStyle": "" }
      },

      {
         "name": "STRING LITERAL",
         "scope": [
            "string",
            "string.quoted",
            "string punctuation",
            "punctuation.definition.string"
         ],
         "settings": { "foreground": "#C3C3C3", "fontStyle": "" }
      },

      {
         "name": "STRING TEMPLATE",
         "scope": [
            "string.template",
            "punctuation.definition.string.template",
            "string.template punctuation.definition"
         ],
         "settings": {
            "foreground": "#D0C877",
            "fontStyle": ""
         }
      },

      {
         "name": "LITERAL | STRING TEMPLATE VAR-WRAPPER, i.e. -> ${}`",
         "scope": [
            "string.template meta.template.expression meta.embedded.line variable.other"
         ],
         "settings": {
            "foreground": "#E4A040",
            "fontStyle": "bold"
         }
      },

      {
         "name": "CONSTANT | GENERIC-SELECTOR",
         "scope": ["constant"],
         "settings": {
            "foreground": "#DD3399",
            "fontStyle": ""
         }
      },

      {
         "name": "KEYWORD & OPERATORS | GENERIC SELECTOR",
         "scope": ["keyword", "storage"],
         "settings": { "foreground": "#00A2C4", "fontStyle": "bold" }
      },

      {
         "name": "KEYWORD | EXPRESSIONS",
         "scope": "keyword.operator.expression",
         "settings": { "foreground": "#00A2C4", "fontStyle": "italic" }
      },

      {
         "name": "FUNCTION | GENERIC SELECTOR",
         "scope": "entity.name.function",
         "settings": { "foreground": "#EE3377", "fontStyle": "" }
      },

      {
         "name": "FUNCTION | FUNCTION-MEMBERS (aka methods)",
         "scope": "entity.name.function.member",
         "settings": {
            "foreground": "#EE3377",
            "fontStyle": ""
         }
      },

      {
         "name": "PARAMETER",
         "scope": "meta.parameters variable.parameter",
         "settings": { "foreground": "#80CC60", "fontStyle": "" }
      },

      {
         "name": "GENERIC-SCOPE |> GENERIC ENTITY SELECTOR -> CLASS/NAMESPACE/STRUCT/INTERFACE",
         "scope": [
            "entity.name.type",
            "entity.name.class",
            "entity.name.type.class",
            "entity.name.type.namespace",
            "entity.name.type.interface"
         ],
         "settings": { "foreground": "#11BB99", "fontStyle": "" }
      },

      {
         "name": "ANNOTATION | TYPE -> PRIMITIVE",
         "scope": ["support.type.primitive"],
         "settings": {
            "foreground": "#9060FF",
            "fontStyle": "italic"
         }
      },

      {
         "name": "LANGUAGE | SYNTAX_TYPE",
         "scope": "XXXXXXXXXXXXXXXXXXXXXXXXXXXX",
         "settings": {
            "foreground": "#DDAA55",
            "fontStyle": ""
         }
      },
      //
      /*


      ___________________________________________________________________





      -------------------------------------------------------------------
                              DATA-FILE FORMATS
      -------------------------------------------------------------------     */
      {
         "name": "JSON | PROPERTY KEYS",
         "scope": "source.json support.type.property-name.json",
         "settings": {
            "foreground": "#00A0C8",
            "fontStyle": ""
         }
      },

      {
         "name": "LANGUAGE | SYNTAX_TYPE",
         "scope": ["string.quoted.double.json"],
         "settings": {
            "foreground": "#C3C3C3",
            "fontStyle": ""
         }
      },
      /*
      ___________________________________________________________________






      *-----------------------------------------------------------------*
                              DATA-FILE FORMATS
      *-----------------------------------------------------------------*     */
      {
         "name": "FUNDAMENTAL COLOR-SELECTORS | INVALID",
         "scope": "invalid",
         "settings": {
            "fontStyle": "strikethrough",
            "foreground": "#FF5544"
         }
      }
